/**app.wxss**/
.container1, .container2, .container3{
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 
.container1{
  padding: 250rpx 0;
}
.container2{
  padding: 200rpx 0;
}
.container3{
  padding: 50rpx 0;
}
.into1, .into2, .into3, .into4, .into5{
  font-family: 微软雅黑体;
  color: #114514;
  font-weight: bold;
  text-align:center;
}
.into1, .into2 {
  font-size: 45px; 
}
.into3, .into4 {
  font-size: 25px; 
}
.into5 {
  font-size: 15px; 
}
.into1, .into3 {
  margin-top: 55px;
}
.into2, .into4{
  margin-top: 15px;
}
.into5{
  margin-top: 0px;
}
.btn1, .btn2, .btn3{
  height: 80rpx;
  width: 150rpx;
  background-color: #114514;
  color:white;
  font-family: 微软雅黑体;
  font-size: 15px;
  font-weight: bold;
}
.btn1{
  margin-top: 100px;
}
.btn2{
  margin-top: 25px;
}
.btn3{
  margin-top: 0px;
}
text {
  font-size: 10px;
  color: #114514;
  font-family: 微软雅黑体;
  font-weight: bold;
  text-align:center;
}
image{
  width: 90rpx;
  height: 90rpx;
  border:0px solid #114514;
}
.bg {
  position: absolute;
  left: 0;
  bottom: 0;
  display: block;
  width: 100%;
  height: 100%;
  z-index: -9999;
  border:0px;
}
.content-btn-group{
  padding: 10px;
}
.button-hover {
  background-color: RGB(191,98,10);
  color:#fff;
}
/** 添加自定义button点击态样式类**/
.other-button-hover {
  background-color: blue;
}
.inputshow1, .inputshow2{
  background-color: white;
  border:1px solid #114514;
  width: 200rpx;
  height: 20rpx;
}
.inputshow1{
  margin-top: 10px;
}
.inputshow2{
  margin-top: 0px;
}