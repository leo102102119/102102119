def getcid(word):#通过输入BV号，获取cid号
    url='https://api.bilibili.com/x/player/pagelist?bvid='+word+'&jsonp=jsonp'#word就是输入的BV号
    myheaders={'Accept':'text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,*/*;q=0.8',
            "User-Agent":"Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/71.0.3578.98 Safari/537.36"}
    response = requests.get(url,headers=myheaders)
    #print(url)#输出获取cid的网址
    return response.content.decode('unicode-escape')#（line11——line16）编码转换时，先将以Unicode为中间编码，先用decode转换成Unicode编码
def getdanmu(cidnumber):#通过cid，获取弹幕
    url='http://comment.bilibili.com/{}.xml'.format(cidnumber)
    #url='https://comment.bilibili.com/';+cidnumber+'.xml'#网页信息
    response = requests.get(url)#获取html网页
    response.encoding='utf-8'#编码格式为UTF-8格式，将Unicode编码转变为utf-8的编码（用非Unicode的编码格式进行编码会出错
    # ）（line11——line16）
    response=response.text#将response格式变成字符串
    pattern=re.compile('<d.*?>(.*?)</d>')#设定找出弹幕的格式，文本内容：<d>(弹幕)</d>
    data=pattern.findall(response)#找出弹幕
    return data
def txt_write(tablelist):#tablelist是字典
    with open('danmu.txt','w',encoding='utf-8')as fp:
        for k,v in tablelist.items():#读取字典中的key值和value值
            fp.write(k+':'+str(v))#value值此时是int，要写入文档必须要是str
            fp.write('\n')#空行
b=[]
#words=['BV1EE411T7Vc','BV1ZJ41137Go']
for i in range(lv):
    #word = input("请输入视频号:")  # 输入视频号进入网页
    getcids = getcid(video_urls[i])  # 进入https://api.bilibili.com/x/player/pagelist?bvid=+BV号+&jsonp=jsonp
    cid = re.findall('"cid":(\d{6,10})', getcids)  # 通过正则表达式获取cid，获取6——10长度的数字,此时是列表
    cid = cid[0]  # 然后取出cid,变成整数值
    danmu = getdanmu(str(cid))  # 将int变成str
    b=b+danmu
    # BV1Rg411d7Ve
d = {}
for i in b:
    d[i] = d.get(i, 0) + 1
a = dict(sorted(d.items(), key=lambda x: x[1], reverse=True))
#print(a)
# x[0]即用“键”排序，x[1]即用“值”排序；返回结果为新的列表，可以通过dict()函数转为字典格式。
#txt_write(a)
a1 = [(key,value) for key,value in a.items()]
a2=len(a1)
print(a1,a2)
if(a2>20):
    a2=20
for i in range(a2):
    print(a1[i])
#print(b)
zymk=''.join(i for i in b) #将所有弹幕拼接在一起
word1=list(jieba.cut(zymk))  ###利用jieba库将弹幕按词进行切分
word1=[i for i in word1 if len(i)>1] ###挑出长度大于1的词语（为去除诸如？，哈，啊等字符）
wc=wordcloud.WordCloud(height=2000,width=2500,font_path='simsun.ttc')#利用wordcloud库定义词云图片的信息
wc.generate(' '.join(word1))   ##生成图片
print(wc)
plt.imshow(wc)
plt.show()
